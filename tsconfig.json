{
    "compilerOptions": {
      "target": "es2016",
      "moduleDetection": "force",
      "module": "commonjs",
      "outDir": "./dist",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "noImplicitAny": true,
      "skipLibCheck": true
    },
    "include": ["src/**/*"]
  }
  